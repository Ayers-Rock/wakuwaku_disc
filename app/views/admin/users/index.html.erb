<h2>ユーザー一覧</h2>
    <table class="table">
        <% @users. each do |user| %>
            <thead>
                <tr>
                    <th>名前</th>
                    <th>名前(カナ)</th>
                    <th>郵便番号</th>
                    <th>住所</th>
                    <th>電話番号</th>
                    <th>メールアドレス</th>
                    <th>/</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><% user.first_name %><% user.last_name %></td>
                    <td><% user.first_kana_name %><% user.last_kana_name %></td>
                    <td><% user.postal_code %></td>
                    <td><% user.prefecture %><% user.city_address %><% user.building %></td>
                    <td><% user.tel_number %></td>
                    <td><% user.email %></td>
                    <td><%= link_to '編集', edit_admin_user_path(user.id) %></td>
                <tr>
            </tbody>
        <% end %>
    </table>