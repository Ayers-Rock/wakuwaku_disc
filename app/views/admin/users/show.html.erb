<h1>Admin::Users#show</h1>
<p>Find me in app/views/admin/users/show.html.erb</p>
<div class="container">
    <div class="row">
        <div class="col-md-6">
                <div class ="field">
                    <%= @user.first_name %><%= @user.last_name %>
                </div>
                <div class ="field">
                    <%= @user.first_kana_name %><%= @user.last_kana_name %>
                </div>
                <div class ="field">
                    <%= @user.postal_code %>
                </div>
                <div class ="field">
                    <%= @user.prefecture %><%= @user.city_address %><%= @user.building %>
                </div>
                <div class ="field">
                    <%= @user.tel_number %>
                </div>
                <div class ="field">
                    <%= @user.email %>
                </div>
                <div class ="field">
                    <%= @user.password %>
                </div>
                <div class="botton">
                <%= link_to '編集', edit_admin_user(@user.id) %>""|<%= link_to '退会', admin_user_path(@user.id) method: :delete %>
                </div>
        <div class="col-md-6">
                <table class="table">
                    <thead>
                        <tr>
                            <th>名前</th>
                            <th>登録住所</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><%= @address.first_name %><%= @address.last_name %></td>
                            <td><%= @address.prefecture %><%= @address.city_address %><%= @address.building  %>
                        </tr>
                    </tbody>
                </table>
        </div>