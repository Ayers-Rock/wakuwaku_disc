<h1>Items#ranking</h1>
<p>Find me in app/views/items/ranking.html.erb</p>
<h1>ランキング</h1>
    <div class="container">
            <div class="row">
                <div class="col-md-2">
                    <div class="ui-tab">
                        <ul>
                            <li>週間ランキング</li>
                            <li>いいねランキング</li>
                            <li>月間ランキング</li>
                        <ul>
                </div>
            </div>
            <div class="row">
                <div class="col-md-10">
                    <div class="flagment-1">
                        <div class="row">
                            <div class="col-md-2">
                                <% users.each_with_index do |user, i|
                                    puts "#{i+1}: #{user.name}" end %>
                            </div>
                            <div class="col-md-2">
                                <%= attachment_image_tag @items :jacket_image_id, :format 'jpeg', :size '60*60' %>
                            </div>
                            <div class="col-md-4">
                                <ul>
                                    <li><%= item.title %></li>
                                    <li><%= item.artist %></li>
                                    <li><%= item.price %></li>
                                <ul>
                            </div>
                            <div class="col-md-4">
                                <button><%= link_to 'お気に入り', favorites_path(user_id) %></button>
                                <button><%= link_to 'カートに入れる', cart_items_path(user_id) %><button>
                            </div>
                    </div>
                <% end %>
            </div>
    </div>